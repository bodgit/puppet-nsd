{
  "name": "bodgit-nsd",
  "version": "1.0.0",
  "author": "bodgit",
  "summary": "Puppet Module for managing NSD",
  "license": "Apache-2.0",
  "source": "https://github.com/bodgit/puppet-nsd",
  "project_page": "https://github.com/bodgit/puppet-nsd",
  "issues_url": "https://github.com/bodgit/puppet-nsd/issues",
  "operatingsystem_support": [
